<a href="/view/home">Inicio</a>
<h2>Carrito</h2>
    {{#each products}}
        <h3>{{this.product.code}} - {{this.product.title}}</h3>
        <ul>
            <li>{{this.product.description}}</li>
            <li>Precio: ${{this.product.price}}</li>
            <li>Cantidad: {{this.quantity}}</li>
        </ul>
    {{/each}}
<h3>Total: ${{total}}</h3>